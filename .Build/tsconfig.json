{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es5",
			"es2015",
			"dom"
		],
		"sourceMap": false,
		"removeComments": false,
		"alwaysStrict": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"pretty": true,

		"typeRoots": [
			"node_modules/@types",
			"types"
		]
	}
}
